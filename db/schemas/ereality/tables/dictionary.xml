<?xml version="1.0"?>
  <table name="dictionary" type="normal">
  <columns>
    <column name="id"   type="number(8)"    nullable="n"/>
    <column name="name" type="varchar2(64)" nullable="n"/>
  </columns>
  <constraints>
    <constraint name="pk_dictionary"  type="primary key" columns_list="id"/>
    <constraint name="unq_dictionary" type="unique"      columns_list="name"/>
  </constraints>
</table>